
%p.ui.info.message
  = "Vous avez trouvé  #{@result_more_sponsorship.count} utilisateur(s) qui ont parrainés le plus de monde"
%table.ui.single.line.table
  %thead
    %tr
      %th Fullname
      %th sponsorship total
  %tbody
    - @result_more_sponsorship.each do |l|
      %tr
        %td
          = l[:user]
        %td
          = l[:count]
